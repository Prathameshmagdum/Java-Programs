public class EvenOddInArray
{
	public static void main(String args[])
	{
		int a[]= {5,6,7,8,2,6};
		
		System.out.println("The Even elements in an array:");
		for(int i=0;i<a.length;i++)
		{
			if(a[i]%2==0)
			{
				System.out.println(a[i]);
			}
		}
		System.out.println("The Odd elements in an array:");
		for (int i=0;i<a.length;i++)
		{
			if(a[i]%2!=0)
			{
				System.out.println(a[i]);
			}
		}
	}
}